  "dependencies": {
    "@sinclair/typebox": "^0.28.20",
    "@swc/jest": "^0.2.26",
    "ajv": "^8.12.0",
    "dotenv": "^16.3.1",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "module-alias": "^2.2.3",
    "pino-pretty": "^10.0.0",
    "ts-node": "^10.9.1"
